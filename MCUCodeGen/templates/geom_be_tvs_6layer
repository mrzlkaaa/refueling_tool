******слой 4*******
be b1 -2 z2 -z3 U b1 -4 3 z2 -z3 U b1 5 z2 -z3                          # m=1392
******слой 5*******
be b1 -2 z1 -z2 U b1 -4 3 z1 -z2 U b1 5 z1 -z2                          # m=1428
******слой 6*******
be b1 -2 zm -z1 U b1 -4 3 zm -z1 U b1 5 zm -z1                                      # m=1464


******  BE36    *******
******слой 1*******
be b4 -2 z5 U b4 -4 3 z5 U b4 5 z5                          # m=1285
******слой 2*******
be b4 -2 z4 -z5 U b4 -4 3 z4 -z5 U b4 5 z4 -z5                          # m=1321
******слой 3*******
be b4 -2 z3 -z4 U b4 -4 3 z3 -z4 U b4 5 z3 -z4                          # m=1357
******слой 4*******
be b4 -2 z2 -z3 U b4 -4 3 z2 -z3 U b4 5 z2 -z3                          # m=1393
******слой 5*******
be b4 -2 z1 -z2 U b4 -4 3 z1 -z2 U b4 5 z1 -z2                          # m=1429
******слой 6*******
be b4 -2 zm -z1 U b4 -4 3 zm -z1 U b4 5 zm -z1                                      # m=1465

zaz 2 -3 U 4 -5                                     # m=1240
END
ENDL


*******************Прототип бериллиевого блока с пробками d90 и d110*********************
LCELL BB2
RPP N1  -rbb1,rbb1  -rbb1,rbb1  0,H
RCZ N2   0,0,0  H  rbb2
RCZ N3   0,0,0  H  rbb6
END
be   1 -2  U  3                                                                                    # m=1201
zaz  2 -3                                                                                          # m=1240
END
ENDL

**************************Прототип бериллиевого блока для ВТС****************************
LCELL BE37
